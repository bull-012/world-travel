# This file should be copied to local.properties and customized for your environment
# local.properties is git-ignored and should NOT be committed to version control

# SDK location (auto-generated by Flutter)
sdk.dir=/path/to/Android/sdk
flutter.sdk=/path/to/flutter

# MapBox Secret Token for downloading the SDK
# Get your secret token from: https://account.mapbox.com/
# This token is different from the public token used in the app
MAPBOX_DOWNLOADS_TOKEN=sk.your_secret_token_here